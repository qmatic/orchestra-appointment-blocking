{
  "name": "appointment-blocking",
  "version": "2.0.0",
  "build": "3",
  "license": "MIT",
  "scripts": {
    "start": "npm run ng-config-dev && ng serve --host 0.0.0.0 --proxy-config proxy.config.json",
    "startlocal": "npm run ng-config-dev && ng serve --proxy-config proxy.config.localhost.json",
    "build": "ng build --prod",
    "test": "ng test",
    "build-war-dev": "npm run ng-config-dev && ng build && gulp buildWarProperties",
    "build-war-prod": "gulp setAppVersion && npm run ng-config-prod && ng build --prod --base-href /appointmentblocking/ && gulp buildWarProperties",
    "build-artifactory": "gulp setAppVersion && npm run ng-config-prod && ng build --prod --base-href /appointmentblocking/ && gulp buildArtifactory",
    "deploy-war-dev": "npm run ng-config-dev && ng build && gulp deployWarProperties",
    "deploy-war-prod": "npm run ng-config-prod && ng build --prod --base-href /appointmentblocking/ && gulp deployWarProperties",
    "deploy-lang": "gulp deploy:lang",
    "deploy-artifactory": "npm run ng-config-prod && ng build --prod --base-href /appointmentblocking/ && gulp buildArtifactoryDeploy",
    "ng-config-dev": "node build-configuration/build-config.js dev",
    "ng-config-prod": "node build-configuration/build-config.js prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.10",
    "@angular/cdk": "^11.2.9",
