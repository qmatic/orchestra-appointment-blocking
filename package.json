    "@angular/localize": "^11.2.10",
    "@ngrx/store-devtools": "^11.1.0",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
